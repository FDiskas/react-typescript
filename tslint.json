{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-airbnb",
        "tslint-config-prettier"
    ],
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-empty-interface": false,
        "member-access": false,
        "strict-boolean-expressions": false,
        "variable-name": false
    }
}
